{
  "compilerOptions": {
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "bundler",
    "types": ["vite/client"],
    "jsx": "react-jsx",
    "paths": {
      "~/*": ["./app/*"]
    },
    "rootDirs": [".", "./.react-router/types"],
    "noEmit": true
  },
  "include": ["app/**/*", "react-router.config.ts", "vite.config.ts"],
  "exclude": ["build"]
}
